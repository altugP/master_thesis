 doi = {10.1145/2632048.2636082},
 edition = {nan},
 editor = {nan},
 isbn = {9781450329682},
 issn = {nan},
 issue_date = {nan},
 journal = {nan},
 keywords = {self-report, mobile health, mobile application, intervention, interruption, EMA},
 location = {Seattle, Washington},
 month = {nan},
 note = {nan},
 number = {nan},
 numpages = {12},
 pages = {909–920},
 publisher = {Association for Computing Machinery},
 series = {UbiComp '14},
 title = {Assessing the availability of users to engage in just-in-time intervention in the natural environment},
