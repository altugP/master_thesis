 edition = {nan},
 editor = {nan},
 isbn = {nan},
 issn = {nan},
 issue_date = {December 2018},
 journal = {Proc. ACM Interact. Mob. Wearable Ubiquitous Technol.},
 keywords = {adherence, clustering algorithm, college students, engagement, longitudinal study, mHealth, multiple health behaviors},
 location = {nan},
 month = {dec},
 note = {nan},
 number = {4},
 numpages = {26},
 pages = {nan},
 publisher = {Association for Computing Machinery},
 series = {nan},
 title = {Is More Always Better? Discovering Incentivized mHealth Intervention Engagement Related to Health Behavior Trends},
 url = {https://doi.org/10.1145/3287031},
